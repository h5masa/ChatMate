<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Edittext Util -->
    <style name="edittext_util">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@android:color/white</item>
        <item name="android:textSize">25dp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
    </style>
    <string name="username_hint">Username</string>
    <string name="email_hint">Email</string>
    <string name="password_hint">Password</string>

    <!-- Button Util-->
    <style name="button_util">
        <item name="android:textStyle">bold</item>
        <item name="android:elevation" tools:targetApi="lollipop">10dp</item>
        <item name="android:stateListAnimator" tools:targetApi="lollipop">@null</item>
    </style>
    <string name="register_string">Register</string>

    <string name="alreadyhaveaccount_string">Already have an account?</string>


</resources>